
<div class="container pt-5 ">
<table id="dtBasicExample" class="table table-striped table-bordered table-sm mt-2" cellspacing="0" width="100%">
  <thead>
    <tr class="text-center"> 
     
      <th  class="th-sm pt-3">User

      </th>
      <th class="th-sm">Order ID

      </th>
      <th class="th-sm">Total Price

      </th>
      <th class="th-sm">Ordered On

      </th>
      <th class="th-sm">Status

      </th>
      <th class="th-sm">

      </th>
     
    </tr>
  </thead>
  <tbody>
   <% orderitems.reverse().forEach((items)=>{ %> 
    <tr class="text-center">
      <td class="p-4"><%= items.orders.firstname  %>  </td>
      <td><%= items.orders._id %> </td>
      <td><%=  items.orders.totalPrice  %> </td>
      <td><%= items.orders.createdAt.getDate()+":"+ items.orders.createdAt.getMonth()+":"+items.orders.createdAt.getFullYear() %> </td>
     <% if(items.orders.paymentStatus==1){ %> 
      <td class="font-weight-bold text-white" style="background-color:rgb(39, 250, 32) ;">SUCCESS</td>
      <% }else{ %> 
        <td class="font-weight-bold text-white" style="background-color:rgb(255, 31, 15) ;">PENDING</td>
        <% } %> 

      <td> <a href="/admin/viewmore/<%=items.orders._id  %> " class="text-decoration-none  font-weight-bold" href=""> view more</a>
      </td>
      
    </tr>
    <% }) %> 
   </tbody>
</table>

</div>











